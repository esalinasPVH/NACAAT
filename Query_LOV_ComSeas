let
    RelativePath = Excel.CurrentWorkbook(){[Name="RelativePath"]}[Content]{0}[Column1],
    Source = Excel.Workbook(File.Contents(RelativePath & "Product Data Governance Worksheet.xlsm"), null, true),
    #"Season-Comm(SAP)_Sheet" = Source{[Item="Season-Comm(SAP)",Kind="Sheet"]}[Data],
    #"Removed Top Rows" = Table.Skip(#"Season-Comm(SAP)_Sheet",1),
    #"Promoted Headers" = Table.PromoteHeaders(#"Removed Top Rows", [PromoteAllScalars=true]),
    #"Changed Type" = Table.TransformColumnTypes(#"Promoted Headers",{{"MDM Commercial Season Year ", type text}, {"Code", type text}, {"Season Year", Int64.Type}, {"S/4  Tech Pack Season Code", type text}, {"S/4 Season Description", type text}, {"Valid-From", type date}, {"Valid-To", type date}, {"Status", type text}, {"Date field", type date}, {"Person", type text}, {"Centric", type any}, {"SAP", type text}, {"MDM", type text}, {"XPC", type any}, {"JDA", type any}, {"ACS", type any}, {"Heritage Sportwear", type any}, {"Calvin Klein", type any}, {"Dress Furnishings Group", type any}, {"The Underwear Group", type any}, {"Swim", type any}, {"Tommy Hilfiger", type any}, {"THJ", type text}, {"CK Asia", type text}}),
    #"Filtered for Active Status" = Table.SelectRows(#"Changed Type", each ([Status] = "Active")),
    #"Removed Other Columns" = Table.SelectColumns(#"Filtered for Active Status",{"MDM Commercial Season Year "})
in
