let
    RelativePath = Excel.CurrentWorkbook(){[Name="RelativePath"]}[Content]{0}[Column1],
    Source = Excel.Workbook(File.Contents(RelativePath & "Product Data Governance Worksheet.xlsm"), null, true),
    SampleSizeIndicator_Sheet = Source{[Item="SampleSizeIndicator",Kind="Sheet"]}[Data],
    #"Removed Top Rows" = Table.Skip(SampleSizeIndicator_Sheet,1),
    #"Promoted Headers" = Table.PromoteHeaders(#"Removed Top Rows", [PromoteAllScalars=true]),
    #"Changed Type" = Table.TransformColumnTypes(#"Promoted Headers",{{"Value", type text}, {"Code", type text}, {"Description", type text}, {"Status", type text}, {"Date field", type date}, {"Person", type text}, {"Comment/Follow Up", type text}, {"Centric", type text}, {"SAP", type text}, {"MDM", type text}, {"XPC", type text}, {"JDA", type text}, {"ACS", type text}, {"Heritage Sportwear", type text}, {"Calvin Klein", type text}, {"Dress Furnishings Group", type text}, {"The Underwear Group", type text}, {"Swim", type text}, {"Tommy Hilfiger", type text}, {"THJ", type text}, {"CK Asia", type text}}),
    #"Filtered to Active" = Table.SelectRows(#"Changed Type", each ([Status] = "Active")),
    #"Removed Other Columns" = Table.SelectColumns(#"Filtered to Active",{"Value"})
in
    #"Removed Other Columns"
