let
    RelativePath = Excel.CurrentWorkbook(){[Name="RelativePath"]}[Content]{0}[Column1],
    Source = Excel.Workbook(File.Contents(RelativePath & "excel.xlsx"), null, true),
    Sheet1_Sheet = Source{[Item="Sheet1",Kind="Sheet"]}[Data],
    #"Promoted Headers" = Table.PromoteHeaders(Sheet1_Sheet, [PromoteAllScalars=true]),
    #"Changed Type" = Table.TransformColumnTypes(#"Promoted Headers",{{"Commercialized Style Number(*)", type any}, {"Legacy Style Number", type text}, {"Style Number(*)", type text}, {"Description", type text}, {"Short Style Description", type text}, {"Color Code(*)", type text}, {"NRF", type text}, {"Colorway Short Description", type text}, {"Color Group/Hue", type text}, {"Colorway Active", type logical}, {"Pattern#", type text}, {"TP Season Year(*)", type text}, {"Tech Pack Stage", type text}, {"Business Group", type text}, {"Brand", type text}, {"SubBrand", type text}, {"Merchandise Sector", type text}, {"Gender", type text}, {"Product Type", type text}, {"Class", type text}, {"SubClass", type text}, {"SMU Customer", type any}, {"Length", type text}, {"Mfg Units", Int64.Type}, {"Pack Type", type text}, {"Fit", type text}, {"Packaging", type any}, {"Program", type text}, {"DTS Channel", type any}, {"DTS Owner", type any}, {"DTS Selling Countries", type any}, {"Size Category", type text}, {"Size Range", type text}, {"Size Range Offered", type text}, {"Adopted Sizes(*)", type any}, {"Collection(*)", type any}, {"Commercial Season", type any}, {"Theme", type any}, {"Product Segmentation", type any}, {"Forecast Code", type any}, {"Pack Method", type any}, {"Delivery Plant", type any}, {"Wholesale Price", type any}, {"Wholesale Price Currency", type any}, {"MSRP", type any}, {"MSRP Currency", type any}, {"LDP", type any}, {"LDP Currency", type any}, {"Sample/Bulk Indicator", type any}, {"SYSTEM USE ONLY", type text}}),
    #"Filtered Color Level" = Table.SelectRows(#"Changed Type", each not Text.EndsWith([SYSTEM USE ONLY], "(STYLE)")),
    #"Added Key" = Table.AddColumn(#"Filtered Color Level", "Key", each Text.Split([SYSTEM USE ONLY],"-"){0}),
    #"Removed Non-Color Columns" = Table.SelectColumns(#"Added Key",{"Color Code(*)", "NRF", "Colorway", "Colorway Short Description", "Color Group/Hue", "Colorway Active", "Pattern#", "Product Segmentation","SYSTEM USE ONLY", "Key"}),
    #"Filltered Inactive Colors" = Table.SelectRows(#"Removed Non-Color Columns", each ([Colorway Active] <> false)),
    #"Removed Colorway Active" = Table.RemoveColumns(#"Filltered Inactive Colors",{"Colorway Active"})
in
    #"Removed Colorway Active"
