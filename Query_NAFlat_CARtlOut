let
    Source = #"Query_NAFlat",
    #"Filtered TH" = Table.SelectRows(Source, each ([Business Group] = "Tommy Hilfiger")),
    #"Removed Collection" = Table.RemoveColumns(#"Filtered TH",{"Collection(*)"}),
    #"Added Default Collection" = Table.AddColumn(#"Removed Collection", "Collection(*)", each "Canada Tommy Hilfiger Retail Outlet"),
    #"Removed Dlvy Plnt" = Table.RemoveColumns(#"Added Default Collection",{"Delivery Plant"}),
    #"Added Default Dlvy Plnt" = Table.AddColumn(#"Removed Dlvy Plnt", "Delivery Plant", each "MONTREAL DC RETAIL"),
    #"Removed Whs Currency" = Table.RemoveColumns(#"Added Default Dlvy Plnt",{"Wholesale Price Currency"}),
    #"Added Default Whs Currency" = Table.AddColumn(#"Removed Whs Currency", "Wholesale Price Currency", each "United States Dollar"),
    #"Removed MSRP Currency" = Table.RemoveColumns(#"Added Default Whs Currency",{"MSRP Currency"}),
    #"Added MSRP Currency" = Table.AddColumn(#"Removed MSRP Currency", "MSRP Currency", each "Canadian Dollar"),
    #"Reordered Final Columns" = Table.ReorderColumns(#"Added MSRP Currency",{"Commercialized Style Number(*)", "Legacy Style Number", "Style Number(*)", "Description", "Short Style Description", "Color Code(*)", "NRF", "Colorway Short Description", "Color Group/Hue", "Colorway Active", "Pattern#", "TP Season Year(*)", "Tech Pack Stage", "Business Group", "Brand", "SubBrand", "Merchandise Sector", "Gender", "Product Type", "Class", "SubClass", "SMU Customer", "Length", "Mfg Units", "Pack Type", "Fit", "Packaging", "Program", "DTS Channel", "DTS Owner", "DTS Selling Countries", "Size Category", "Size Range", "Size Range Offered", "Adopted Sizes(*)", "Collection(*)", "Commercial Season", "Theme", "Product Segmentation", "Forecast Code", "Pack Method", "Delivery Plant", "Wholesale Price", "Wholesale Price Currency", "MSRP", "MSRP Currency", "Sample/Bulk Indicator", "SYSTEM USE ONLY"}),
    #"Split DTS Channel" = Table.ExpandListColumn(Table.TransformColumns(Table.TransformColumnTypes(#"Reordered Final Columns", {{"DTS Channel", type text}}, "en-US"), {{"DTS Channel", Splitter.SplitTextByDelimiter(";", QuoteStyle.Csv), let itemType = (type nullable text) meta [Serialized.Text = true] in type {itemType}}}), "DTS Channel"),
    #"Changed DTS Channel Type" = Table.TransformColumnTypes(#"Split DTS Channel",{{"DTS Channel", type text}}),
    #"Filtered DTS Channel" = Table.SelectRows(#"Changed DTS Channel Type", each ([DTS Channel] = "Retail")),
    #"Split DTS Selling Countries" = Table.ExpandListColumn(Table.TransformColumns(#"Filtered DTS Channel", {{"DTS Selling Countries", Splitter.SplitTextByDelimiter(";", QuoteStyle.Csv), let itemType = (type nullable text) meta [Serialized.Text = true] in type {itemType}}}), "DTS Selling Countries"),
    #"Changed DTS Selling Countries Type" = Table.TransformColumnTypes(#"Split DTS Selling Countries",{{"DTS Selling Countries", type text}}),
    #"Filtered DTS Selling Countries" = Table.SelectRows(#"Changed DTS Selling Countries Type", each ([DTS Selling Countries] = null or [DTS Selling Countries] = "Canada"))
in
