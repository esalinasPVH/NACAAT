let
    RelativePath = Excel.CurrentWorkbook(){[Name="RelativePath"]}[Content]{0}[Column1],
    Source = Excel.Workbook(File.Contents(RelativePath & "excel.xlsx"), null, true),
    Sheet1_Sheet = Source{[Item="Sheet1",Kind="Sheet"]}[Data],
    #"Promoted Headers" = Table.PromoteHeaders(Sheet1_Sheet, [PromoteAllScalars=true]),
    #"Changed Type" = Table.TransformColumnTypes(#"Promoted Headers",{{"Commercialized Style Number(*)", type any}, {"Legacy Style Number", type text}, {"Style Number(*)", type text}, {"Description", type text}, {"Short Style Description", type text}, {"Color Code(*)", type text}, {"NRF", Int64.Type}, {"Colorway Short Description", type text}, {"Color Group/Hue", type text}, {"Colorway Active", type logical}, {"Pattern#", type text}, {"TP Season Year(*)", type text}, {"Tech Pack Stage", type text}, {"Business Group", type text}, {"Brand", type text}, {"SubBrand", type text}, {"Merchandise Sector", type text}, {"Gender", type text}, {"Product Type", type text}, {"Class", type text}, {"SubClass", type text}, {"SMU Customer", type any}, {"Length", type text}, {"Mfg Units", Int64.Type}, {"Pack Type", type text}, {"Fit", type text}, {"Packaging", type any}, {"Program", type text}, {"DTS Channel", type any}, {"DTS Owner", type any}, {"DTS Selling Countries", type any}, {"Size Category", type text}, {"Size Range", type text}, {"Size Range Offered", type text}, {"Adopted Sizes(*)", type any}, {"Collection(*)", type any}, {"Commercial Season", type any}, {"Theme", type any}, {"Product Segmentation", type any}, {"Forecast Code", type any}, {"Pack Method", type any}, {"Delivery Plant", type any}, {"Wholesale Price", type any}, {"Wholesale Price Currency", type any}, {"MSRP", type any}, {"MSRP Currency", type any}, {"LDP", type any}, {"LDP Currency", type any}, {"Sample/Bulk Indicator", type any}, {"SYSTEM USE ONLY", type text}}),
    #"Filtered Style Level" = Table.SelectRows(#"Changed Type", each Text.EndsWith([SYSTEM USE ONLY], "(STYLE)")),
    #"Added Key" = Table.AddColumn(#"Filtered Style Level", "Key", each Text.Split([SYSTEM USE ONLY],"-"){0}),
    #"Removed Non-Style Columns" = Table.RemoveColumns(#"Added Key",{"Color Code(*)", "NRF", "Colorway Short Description", "Color Group/Hue", "Colorway Active", "Pattern#", "Product Segmentation", "SYSTEM USE ONLY"}),
    #"Removed Commercial Season" = Table.RemoveColumns(#"Removed Non-Style Columns",{"Commercial Season"}),
    #"Added Commercial Season" = Table.AddColumn(#"Removed Commercial Season", "Commercial Season", each Text.Split([#"TP Season Year(*)"],"("){0}),
    #"Trimmed Commercial Season" = Table.TransformColumns(#"Added Commercial Season",{{"Commercial Season", Text.Trim, type text}}),
    #"Removed Pack Method" = Table.RemoveColumns(#"Trimmed Commercial Season",{"Pack Method"}),
    #"Added Pack Method" = Table.AddColumn(#"Removed Pack Method", "Pack Method", each [Packaging]),
    #"Removed Sample Bulk" = Table.RemoveColumns(#"Added Pack Method",{"Sample/Bulk Indicator"}),
    #"Added Sample Bulk" = Table.AddColumn(#"Removed Sample Bulk", "Sample/Bulk Indicator", each "B"),
    #"Removed Commercial Style" = Table.RemoveColumns(#"Added Sample Bulk",{"Commercialized Style Number(*)"}),
    #"Added Commercial Style" = Table.AddColumn(#"Removed Commercial Style", "Commercialized Style Number(*)", each [Legacy Style Number]),
    #"Removed LDP Columns" = Table.RemoveColumns(#"Added Commercial Style",{"LDP", "LDP Currency"})
in
    #"Removed LDP Columns"
