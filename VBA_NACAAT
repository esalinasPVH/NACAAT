Sub Create_Size_Validation()

    'Source: https://stackoverflow.com/questions/22956604/how-to-add-data-validation-to-a-cell-using-vba

    ActiveWorkbook.Connections("Query - Query_Size_Map").Refresh
    
    Create_SourceSizeKey_Name
    
    Dim dstSheet As Worksheet
    Dim dstRange As Range
    
    Sheets("NA Commercialization Worksheet").Activate
    Set dstSheet = ThisWorkbook.Worksheets("NA Commercialization Worksheet")
    Set dstRange = dstSheet.Range("AH3", Range("AH3").End(xlDown)).Offset(, 2)
    
    With dstRange.Validation
        .Delete 'delete previous validation
        .Add Type:=xlValidateList, AlertStyle:=xlValidAlertStop, _
        Formula1:="=OFFSET(INDEX(SourceSizeKey,1,),MATCH(AX3,SourceSizeKey,0)-1,1,COUNTIF(SourceSizeKey,AX3),1)"
        .InCellDropdown = True
    End With

End Sub


Sub Create_SourceSizeKey_Name()
 
    'Source: https://powerspreadsheets.com/vba-create-named-range/
 
    Worksheets("Sheet_Size_Map").Visible = xlSheetVisible
    Sheets("Sheet_Size_Map").Activate
 
    'declare object variables to hold references to worksheet containing cell range, and cell range itself
    Dim myWorksheet As Worksheet
    Dim myNamedRange As Range
 
    'declare variable to hold defined name
    Dim myRangeName As String
 
    'identify worksheet containing cell range, and cell range itself
    Set myWorksheet = ThisWorkbook.Worksheets("Sheet_Size_Map")
    
    Set myNamedRange = myWorksheet.Range("A2", Range("A2").End(xlDown))
 
    'specify defined name
    myRangeName = "SourceSizeKey"
 
    'create named range with workbook scope. Defined name and cell range are as specified
    ThisWorkbook.Names.Add name:=myRangeName, RefersTo:=myNamedRange
    
    Worksheets("Sheet_Size_Map").Visible = xlSheetHidden
 
End Sub

Sub Validate_ComSeas()
 
    'Source: https://stackoverflow.com/questions/22956604/how-to-add-data-validation-to-a-cell-using-vba

    ActiveWorkbook.Connections("Query - Query_LOV_ComSeas").Refresh

    Dim ws As Worksheet
    Dim srcRange As Range, dstRange As Range
    
    Set srcSheet = ThisWorkbook.Worksheets("Sheet_LOV_ComSeas")
    Set srcRange = Range("Query_LOV_ComSeas")
    
    Set dstSheet = ThisWorkbook.Worksheets("NA Commercialization Worksheet")
    Set dstRange = dstSheet.Range("AW3", Range("AW3").End(xlDown)).Offset(, -11)
    
    With dstRange.Validation
        .Delete 'delete previous validation
        .Add Type:=xlValidateList, AlertStyle:=xlValidAlertStop, _
        Formula1:="='" & srcSheet.name & "'!" & srcRange.Address
        .InCellDropdown = True
    End With
 
End Sub

Sub Validate_Theme()
 
    'Source: https://stackoverflow.com/questions/22956604/how-to-add-data-validation-to-a-cell-using-vba

    ActiveWorkbook.Connections("Query - Query_LOV_Theme").Refresh

    Dim ws As Worksheet
    Dim srcRange As Range, dstRange As Range
    
    Set srcSheet = ThisWorkbook.Worksheets("Sheet_LOV_Theme")
    Set srcRange = Range("Query_LOV_Theme")
    
    Set dstSheet = ThisWorkbook.Worksheets("NA Commercialization Worksheet")
    Set dstRange = dstSheet.Range("AW3", Range("AW3").End(xlDown)).Offset(, -10)
    
    With dstRange.Validation
        .Delete 'delete previous validation
        .Add Type:=xlValidateList, AlertStyle:=xlValidAlertStop, _
        Formula1:="='" & srcSheet.name & "'!" & srcRange.Address
        .InCellDropdown = True
    End With
 
End Sub

Sub Validate_ProdSeg()
 
    'Source: https://stackoverflow.com/questions/22956604/how-to-add-data-validation-to-a-cell-using-vba

    ActiveWorkbook.Connections("Query - Query_LOV_ProdSeg").Refresh

    Dim ws As Worksheet
    Dim srcRange As Range, dstRange As Range
    
    Set srcSheet = ThisWorkbook.Worksheets("Sheet_LOV_ProdSeg")
    Set srcRange = Range("Query_LOV_ProdSeg")
    
    Set dstSheet = ThisWorkbook.Worksheets("NA Commercialization Worksheet")
    Set dstRange = dstSheet.Range("AW3", Range("AW3").End(xlDown)).Offset(, -9)
    
    With dstRange.Validation
        .Delete 'delete previous validation
        .Add Type:=xlValidateList, AlertStyle:=xlValidAlertStop, _
        Formula1:="='" & srcSheet.name & "'!" & srcRange.Address
        .InCellDropdown = True
    End With
 
End Sub

